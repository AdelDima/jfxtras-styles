    <Label layoutX="43.0" layoutY="132.0" styleClass="item-title" text="Normal" />
    <RangeSlider layoutX="43.0" layoutY="154.0"  />

    <Label layoutX="43.0" layoutY="222.0" styleClass="item-title" text="With Tickmarks" />
    <RangeSlider showTickMarks="true" showTickLabels="true" layoutX="43.0" layoutY="244.0"/>

    <Label layoutX="43.0" layoutY="312.0" styleClass="item-title" text="Disabled" />
    <RangeSlider disable="true" layoutX="43.0" layoutY="334.0"/>

    <Label layoutX="313.0" layoutY="132.0" styleClass="item-title" text="vertical" />
    <RangeSlider lowValue="20" highValue="60" orientation="VERTICAL" layoutX="320.0" layoutY="154.0"/>
</AnchorPane>
